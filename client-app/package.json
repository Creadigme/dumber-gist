  "scripts": {
    "css-lint": "sass-lint -c .sass-lint.yml 'src/**/*.scss'",
    "js-lint": "eslint src test",
    "html-lint": "htmlhint -c .htmlhintrc src",
    "lint": "npm run js-lint && npm run css-lint && npm run html-lint",
    "pretest": "npm run lint && npm run build:test",
    "build:test": "gulp clean && cross-env NODE_ENV=test gulp build",
    "coverage": "npm run test && nyc report --reporter=lcov --reporter=text",
    "start": "gulp",
    "build": "gulp clean && gulp build",
    "build:prod": "gulp clean && cross-env NODE_ENV=production gulp build",
    "clear-cache": "gulp clear-cache",
    "test": "browser-do --tap < output/entry-bundle.js | tap-nirvana",
    "browser-test": "npm run build:test && browser-do --tap --browser chrome < output/entry-bundle.js | tap-nirvana"
  },
  "dependencies": {
    "aurelia-bootstrapper": "^2.3.3"
  }
