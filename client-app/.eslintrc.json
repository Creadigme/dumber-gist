{
  "root": true,
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "rules": {
    "no-console": 0,
    "getter-return": 0,
    "no-prototype-builtins": 0
  },
  "env": {
    "es2017": true,
    "browser": true,
    "node": true
  },
  "globals": {
  }
}